            },
            {
                "text": "8. To change the heat, scroll the knob clockwise until the thermostat icon is flickering.",
                "hint": "https://media4.giphy.com/media/v1.Y2lkPTc5MGI3NjExOXN1dWtiYXBsbzdybWM3emdjcnBrN3UxM2V0NHZtejRnNHk4dHduYSZlcD12MV9pbnRlcm5hbF9naWZfYnlfaWQmY3Q9Zw/lgcUUCXgC8mEo/giphy.gif"
            },
            {
                "text": "9. Press the knob once to select the temperature.",
                "hint": "https://media4.giphy.com/media/v1.Y2lkPTc5MGI3NjExOXN1dWtiYXBsbzdybWM3emdjcnBrN3UxM2V0NHZtejRnNHk4dHduYSZlcD12MV9pbnRlcm5hbF9naWZfYnlfaWQmY3Q9Zw/lgcUUCXgC8mEo/giphy.gif"
            },
            {
                "text": "10. Scroll the knob clockwise to change the temperature to 50Â°C.",
                "hint": "https://media4.giphy.com/media/v1.Y2lkPTc5MGI3NjExOXN1dWtiYXBsbzdybWM3emdjcnBrN3UxM2V0NHZtejRnNHk4dHduYSZlcD12MV9pbnRlcm5hbF9naWZfYnlfaWQmY3Q9Zw/lgcUUCXgC8mEo/giphy.gif"
            },
            {
                "text": "11. Press the knob once again to confirm the change.",
                "hint": "https://media4.giphy.com/media/v1.Y2lkPTc5MGI3NjExOXN1dWtiYXBsbzdybWM3emdjcnBrN3UxM2V0NHZtejRnNHk4dHduYSZlcD12MV9pbnRlcm5hbF9naWZfYnlfaWQmY3Q9Zw/lgcUUCXgC8mEo/giphy.gif"
            }
        ]
